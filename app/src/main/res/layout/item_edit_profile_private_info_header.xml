<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingStart="@dimen/dp_16"
    android:paddingTop="50dp"
    android:paddingEnd="@dimen/dp_16"
    android:paddingBottom="26dp">

    <TextView
        android:id="@+id/profile_private_info_header"
        style="@style/Body.014.Narrow.Accent"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/profile_private_info_header"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/profile_private_info_body"
        style="@style/Body.014.Narrow.Light"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_8"
        android:text="@string/profile_private_info_supporting"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/profile_private_info_header" />

    <TextView
        android:id="@+id/edit_profile_contact_us"
        style="@style/Body.012.LightAccent"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/profile_contact_cta"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/profile_private_info_body" />

</androidx.constraintlayout.widget.ConstraintLayout>